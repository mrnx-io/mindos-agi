{
  "name": "mindos",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "10/10 SOTA AGI Agentic Architecture - Persistent, autonomous, multi-model intelligence",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "check": "turbo run check",
    "test": "turbo run test",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "db:migrate": "cd db && ./migrate.sh",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.1.38"
}
