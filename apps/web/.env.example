# MindOS Web UI (Vercel)
NEXT_PUBLIC_APP_NAME=MindOS
# Optional: preset identity ID (skips auto-create)
NEXT_PUBLIC_MINDOS_IDENTITY_ID=
# Optional: default display name for identity creation
NEXT_PUBLIC_MINDOS_DISPLAY_NAME=Artist

# Server-side (do not expose publicly)
RESTATE_INGRESS_URL=https://[YOUR-ENV].env.us.restate.cloud:8080
RESTATE_API_KEY=rsc_...
# Optional overrides
MINDOS_DEFAULT_IDENTITY_NAME=Artist
MINDOS_POLL_INTERVAL_MS=2000
MINDOS_STREAM_TIMEOUT_MS=120000
